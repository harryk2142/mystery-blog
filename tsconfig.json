{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "noUnusedParameters": false,
    "strictNullChecks": true,
    "allowJs": true,
    "noImplicitAny": false,
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}