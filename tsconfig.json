{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "noUnusedParameters": false,
    "strictNullChecks": true,
    "allowJs": true,
    "noImplicitAny": false,
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "paths": {
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ]
    }
  }
}