---
import type { JSX } from "astro/jsx-runtime";
import type { CollectionEntry } from "astro:content";
type ImgAttributes = JSX.ImgHTMLAttributes;
type Props = {
  blogpost: CollectionEntry<"blog">;
} & ImgAttributes;
const { blogpost, ...imgAttrs } = Astro.props;
const src = blogpost.data.heroImage.url;
const alt = blogpost.data.heroImage.alt;
---

<img src={src} alt={alt} {...imgAttrs} loading="lazy" />
